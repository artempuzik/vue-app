{
  "name": "relu-vue3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src"
  },
  "dependencies": {
    "@mathieustan/intercom": "^1.0.0-alpha.0",
    "axios": "^1.5.1",
    "lodash.debounce": "^4.0.8",
    "pinia": "^2.1.6",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.3.4",
    "vue-i18n": "9",
    "vue-router": "4",
    "vue3-toastify": "^0.1.14"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.8",
    "@types/lodash.debounce": "^4.0.9",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "bootstrap": "^5.3.2",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "sass": "^1.68.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "vue"
